{
  "name": "二维码生成器(QRcode)",
  "version": "1.2",
  "manifest_version": 2,
  "description": "二维码生成器，再也不用手写输入超长URL了",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "jquery.qrcode.min.js"]
    }
  ]
}
